<% unless @friends.blank? %>
<div class="">
  <% @friends.each do |friend| %>
  <div class="friends_row">
    <div class="friends_single">
<%= image_tag friend.user.face.url(:s64) %>
      <div><%= friend.user.name %></div>
    </div><div class="clearfloat"></div>
  </div>
  <% end %>
</div>

<% end %>
